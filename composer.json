{
  "name":          "elixant/http",
  "description":   "HTTP Request/Response/Session Management.",
  "type":          "library",
  "authors":       [
    {
      "name":  "Alexander Schmautz",
      "email": "a.schmautz91@gmail.com"
    }
  ],
  "prefer-stable": true,
  "require":       {
    "php":                     "^8.2",
    "elixant/utility": "@dev",
    "symfony/http-foundation": "^7.0",
    "elixant/service-container": "@dev"
  },
  "require-dev":   {
    "phpunit/phpunit":     "^9.5",
    "mockery/mockery":       "^1.4"
  },
  "autoload":      {
    "psr-4": {
      "Elixant\\HTTP\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "repositories":  [
    {
      "type":    "path",
      "url":     "../*",
      "options": {
        "symlink": true
      }
    }
  ],
  "config":        {
    "optimize-autoloader": true
  }
}